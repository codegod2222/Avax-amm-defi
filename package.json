{
  "name": "avalanche-amm-dapp",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "cd packages/contract && npx hardhat test",
    "deploy": "cd packages/contract && npx hardhat run scripts/deploy.ts --network fuji",
    "cp": "npm run cp:typechain && npm run cp:artifact",
      "cp:typechain": "cp -r packages/contract/typechain-types packages/client/",
      "cp:artifact": "cp packages/contract/artifacts/contracts/ERC20Tokens.sol/USDCToken.json packages/contract/artifacts/contracts/ERC20Tokens.sol/JOEToken.json packages/contract/artifacts/contracts/AMM.sol/AMM.json packages/client/utils/",
    "dev": "npm run dev -w packages/client",
    "build": "npm run build -w packages/client",
    "start": "npm run start -w packages/client",
    "lint": "npm run lint -w packages/client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unchain-dev/avalanche-amm-dapp.git"
  },
